    "import pyttsx3\n",
    "\n",
    "# Initialize the text-to-speech engine\n",
    "engine = pyttsx3.init()\n",
    "\n",
    "# Set properties (optional)\n",
    "engine.setProperty('rate', 150)  # Speed of speech\n",
    "\n",
    "# Convert text to speech\n",
    "text = \"Hello, how are you?\"\n",
    "engine.say(text)\n",
    "\n",
    "# Play the speech\n",
    "engine.runAndWait()\n",
